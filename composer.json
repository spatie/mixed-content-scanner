{
    "name": "spatie/mixed-content-scanner",
    "description": "Scan your site for mixed content",
    "keywords": [
        "spatie",
        "mixed-content-scanner"
    ],
    "homepage": "https://github.com/spatie/mixed-content-scanner",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/crawler": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MixedContentScanner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MixedContentScanner\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
